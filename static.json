{
  "root": "build/",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-cache, no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=512000"
    }
  }
}